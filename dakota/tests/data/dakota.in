# Dakota input file
environment
  tabular_data
    tabular_data_file = 'dakota.dat'

method
  multidim_parameter_study
    partitions = 8 8

variables
  continuous_design = 2
    upper_bounds = 2.0 2.0
    lower_bounds = -2.0 -2.0
    descriptors = 'x1' 'x2'

interface
  direct
  analysis_driver = 'rosenbrock'

responses
  response_functions = 1
    response_descriptors = 'r1'
  no_gradients
  no_hessians
