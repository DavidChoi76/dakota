# Dakota input file
environment
  tabular_data
    tabular_data_file = 'dakota.dat'

method
  vector_parameter_study
    final_point = 5 10 15 
    num_steps = 0

variables
  continuous_design = 3
    initial_point = 0 0 0 
    descriptors = 'v1' 'v2' 'v3' 

interface
  fork
  analysis_driver = 'run_model.py'
  parameters_file = 'params.in'
  results_file = 'results.out'
  work_directory
    named 'run'
    directory_tag
    directory_save
  file_save

responses
  response_functions = 2
    response_descriptors = 'Foo' 'Bar' 
  no_gradients
  no_hessians
